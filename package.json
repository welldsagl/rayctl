{
  "name": "rayctl",
  "description": "React Native white label generator",
  "version": "0.0.0",
  "author": "Amedeo Zucchetti @zuccha",
  "bin": {
    "rayctl": "./bin/run"
  },
  "bugs": "https://github.com/welldsagl/rayctl/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^8",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^5",
    "nyc": "^13"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/welldsagl/rayctl",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "rayctl",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:staged"
    }
  },
  "repository": "welldsagl/rayctl",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint",
    "lint:staged": "lint-staged",
    "lint:all": "yarn lint ."
  }
}
